generator client {
  provider = "prisma-client-js"
}

// generator dto {
//   provider = "prisma-class-validator-generator"
//   output   = "../src/global/models"
// }

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  ethAddress String @unique
  email String? @unique
}
